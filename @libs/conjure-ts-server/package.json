{
  "name": "conjure-ts-server",
  "version": "0.0.0",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tshy"
  },
  "dependencies": {
    "conjure-client": "^2.11.0",
    "hono": "^4.6.3",
    "@hono/node-server": "^1.13.1"
  },
  "devDependencies": {
    "@culty/tsconfig": "workspace:*",
    "tshy": "^3.0.2"
  },
  "type": "module",
  "tshy": {
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/main.ts"
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/main.d.ts",
        "default": "./dist/esm/main.js"
      },
      "require": {
        "types": "./dist/commonjs/main.d.ts",
        "default": "./dist/commonjs/main.js"
      }
    }
  },
  "main": "./dist/commonjs/main.js",
  "types": "./dist/commonjs/main.d.ts",
  "module": "./dist/esm/main.js"
}
